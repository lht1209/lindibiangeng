            android:layout_height="wrap_content"
            android:id="@+id/tvjinweidu"
            android:text="N:000°00′00.0″ E:000°00′00.0″"
            android:textSize="20sp"
            android:textColor="#DC143C"
            android:layout_marginLeft="260dp"/>

    </LinearLayout>



