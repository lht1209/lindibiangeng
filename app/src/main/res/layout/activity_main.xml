        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id = "@+id/lbianji">
        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="58dp"
            android:id="@+id/btnshouhui"
            android:layout_weight="1"
            android:onClick="btnshouhui"
            android:clickable="true"
            android:background="@drawable/btnshouhui"
            android:layout_marginBottom="1.3dp"

            />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="58dp"
            android:id="@+id/btndandian"
            android:layout_weight="1"
            android:onClick="btndandian"
            android:clickable="true"
            android:background="@drawable/btndandian"
